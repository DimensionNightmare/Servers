{
    "files.associations": {
        "CMakeLists.txt": "makefile",
        "*.ixx": "cpp"
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
	"C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.indent.lambdaBracesWhenParameter": true,
    "C_Cpp.vcFormat.indent.multiLineRelativeTo": "statementBegin",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "newLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "newLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
    "C_Cpp.vcFormat.indent.caseLabels": true,
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.default.cStandard": "c23",
    "C_Cpp.intelliSenseCachePath": "${workspaceRoot}/out/VsIntelliscene",
    "C_Cpp.default.includePath": [
        "${workspaceRoot}/../Environment/Install/Debug/hiredis/include",
        "${workspaceRoot}/../Environment/Install/Debug/libhv/include",
        "${workspaceRoot}/../Environment/Install/Debug/libpqxx/include",
        "${workspaceRoot}/../Environment/Install/Debug/pbgen/include",
        "${workspaceRoot}/../Environment/Install/Debug/protobuf/include",
        "${workspaceRoot}/../Environment/Install/Debug/redis-plus-plus/include"
    ],
    "C_Cpp.default.systemIncludePath": [
        "${workspaceRoot}/../Environment/Install/Debug/hiredis/include",
        "${workspaceRoot}/../Environment/Install/Debug/libhv/include",
        "${workspaceRoot}/../Environment/Install/Debug/libpqxx/include",
        "${workspaceRoot}/../Environment/Install/Debug/pbgen/include",
        "${workspaceRoot}/../Environment/Install/Debug/protobuf/include",
        "${workspaceRoot}/../Environment/Install/Debug/redis-plus-plus/include"
    ],
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.intelliSenseCacheSize": 51200,
    "cmake.allowCommentsInPresetsFile": true
}